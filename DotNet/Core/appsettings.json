{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Consul": {
    "Address": "http://localhost:8500",
    "ServiceName": "my-microservice",
    "ServiceId": "",
    "Tags": ["api", "microservice"],
    "Port": 5000,
    "ServiceAddress": "localhost",
    "HealthCheckPath": "/health",
    "HealthCheckIntervalSeconds": 10,
    "HealthCheckTimeoutSeconds": 5,
    "DeregisterCriticalServiceAfterSeconds": 30,
    "Enabled": true,
    "Datacenter": "",
    "Token": ""
  },
  "ServiceRegistration": {
    "AutoRegister": true,
    "AutoDeregister": true,
    "RegistrationDelaySeconds": 0,
    "MaxRetryAttempts": 3,
    "RetryDelaySeconds": 5
  }
}

